{
  "name": "simple-react-full-stack",
  "version": "1.0.0",
  "description": "Boilerplate to build a full stack web application using React, Node.js, Express and Webpack.",
  "main": "src/server/index.js",
  "scripts": {
    "start": "npm run build && node src/server/index.js",
    "build": "webpack --mode production --config ./webpack.config.prod.js",
    "bundle": "webpack --watch --mode development --config ./webpack.config.js",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot --config ./webpack.config.js",
    "server": "nodemon src/server/index.js",
    "dev": "concurrently 'npm run server' 'npm run client' 'npm run bundle'"
  },
  "author": "Shiyun Zhang",
  "dependencies": {
    "@grpc/proto-loader": "^0.1.0",
    "@material-ui/core": "^3.5.1",
    "@material-ui/icons": "^3.0.1",
    "axios": "^0.17.1",
    "body-parser": "^1.18.3",
    "consolidate": "^0.15.1",
    "cookie-parser": "1.4.3",
    "ejs": "^2.5.1",
    "express": "^4.16.3",
    "google-protobuf": "^3.0.0",
    "grpc": "^1.11.0",
    "i": "^0.3.6",
    "lodash": "^4.17.13",
    "moment": "^2.23.0",
    "mustache": "^3.0.0",
    "npm": "^6.5.0",
    "react": "^16.5.2",
    "react-burger-menu": "^2.5.4",
    "react-dom": "^16.5.2",
    "react-google-maps": "^9.4.5",
    "react-redux": "5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.0.0",
    "redux": "^3.0.4",
    "redux-form": "^7.4.2",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@grpc/proto-loader": "^0.1.0",
    "babel-eslint": "^10.0.0",
    "babel-loader": "^8.0.0",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^4.0.0",
    "css-loader": "^1.0.0",
    "ejs": "^2.5.1",
    "eslint": "^5.0.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^1.1.11",
    "google-protobuf": "^3.0.0",
    "grpc": "^1.11.0",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "1.18.7",
    "redux-logger": "^2.5.0",
    "style-loader": "^0.23.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.14"
  }
}
